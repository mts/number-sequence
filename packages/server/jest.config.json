{
  "testRegex": ".test\\.js$",
  "roots": ["<rootDir>"],
  "transform": {
    "^.+\\.(js)$": "babel-jest"
  },
  "collectCoverageFrom": ["packages/**/*.js", "!packages/**/*.test.js", "!packages/**/*.story.js", "!packages/**/*.render.js"],
  "moduleNameMapper": {
    "\\.(css|less|scss)$": "identity-obj-proxy"
  },
  "moduleFileExtensions": ["js"]
}
